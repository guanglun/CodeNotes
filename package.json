{
	"name": "codenotes",
	"displayName": "codenotes",
	"description": "Yes",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:codenotes.insertmark",
		"onView:sidebar_marks_now",
		"onView:sidebar_marks_all",
		"onView:sidebar_web"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "codenotes.insertmark",
				"title": "CodeNotes Insert Mark"
			},
			{
				"command": "codenotes.openWebview",
				"title": "CodeNotes Open Web View"
			},
			{
				"command": "codenotes.deleteItem",
				"title": "Delete"
			},
			{
				"command": "codenotes.renameItem",
				"title": "Rename"
			},
			{
				"command": "codenotes.editItem",
				"title": "Edit"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorFocus",
					"command": "codenotes.insertmark",
					"group": "navigation"
				},
				{
					"when": "editorFocus",
					"command": "codenotes.openWebview",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "codenotes.deleteItem",
					"group": "navigation"
				},
				{
					"command": "codenotes.renameItem",
					"group": "navigation"
				},
				{
					"command": "codenotes.editItem",
					"group": "navigation"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "sidebar_codenotes",
					"title": "codenotes",
					"icon": "images/book-mark-line.svg"
				}
			]
		},
		"views": {
			"sidebar_codenotes": [

				{
					"type": "webview",
					"id": "sidebar_web",
					"name": "CodeNotes Menu",
					"contextualTitle": "CodeNotes Menu"
				},{
					"type": "webview",
					"id": "sidebar_mark",
					"name": "CodeNotes Mark Edit",
					"contextualTitle": "CodeNotes Mark Edit"
				},
				{
					"id": "sidebar_marks_now",
					"name": "CodeNotes Marks Now"
				},
				{
					"id": "sidebar_marks_all",
					"name": "CodeNotes Marks All"
				}
				
			]
		},
		"configuration": {
			"type": "object",
			"title": "CodeNotes",
			"properties": {
				"CodeNotes.enableColor": {
					"type": "boolean",
					"default": true,
					"description": "Enable Color CodeNotes"
				},
				"CodeNotes.showHello": {
					"type": "string",
					"default": "Hello",
					"description": "show hello test"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/sqlite3": "^3.1.7",
		"@types/vscode": "^1.54.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"fs": "0.0.1-security",
		"json": "^10.0.0",
		"jsplumb": "^2.15.5",
		"sqlite3": "^5.0.2"
	}
}
